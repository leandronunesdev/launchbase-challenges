{% extends "layout.njk" %}

{% block content %}
<div class="card">
    <table>
        <thead>
            <tr>
                <th>Nome completo</th>
                <th>Acompanhamento</th>
            </tr>
        </thead>
        <tbody>
            {% for teacher in teachers %}
            <tr>
                <td>{{teacher.name}}</td>
                <td>{{teacher.subjects}}</td>
            </tr>
            {% endfor %}
        </tbody>
    </table>
</div>
{% endblock content %}